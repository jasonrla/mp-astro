{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/debug-product/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/orders/create/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/orders/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/payment-status/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/process-payment/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/webhooks/mercadopago/?$",
			"dest": "_render"
		},
		{
			"src": "^/checkout/?$",
			"dest": "_render"
		},
		{
			"src": "^/order/([^/]+?)/confirmation-success/?$",
			"dest": "_render"
		},
		{
			"src": "^/success/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}